class Print 
{
; 	public static void main(String[] args) 
	{
	int n=1;
		for(int i=5;i>=1;i--){
			
			for(int j=i;j>=1;j--){
				System.out.print(n+"\t");
				n++;
			}
			System.out.println();
		}
		n=1;
		for(int i=1;i<=5;i++){
			
			for(int j=i;j>=1;j--){
				System.out.print(n+"\t");
				n++;
			}
			System.out.println();
		}
	}
}
